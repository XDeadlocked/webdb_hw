{
    "manifest_version": 3,
    "name": "Dark Mode Toggle",
    "version": "1.0",
    "description": "Toggle dark mode on any webpage.",
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "16.png",
        "48": "48.png",
        "128": "128.png"
      }
    },
    "permissions": ["activeTab", "scripting"],
    "host_permissions": ["<all_urls>"]
  }
  